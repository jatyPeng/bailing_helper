{
    "name": "bailing/helper",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "php",
        "hyperf"
    ],
    "description": "常见帮助类",
    "autoload": {
        "psr-4": {
            "Bailing\\": "src/"
        },
        "files": [
            "src/Functions.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "HyperfTest\\": "tests"
        }
    },
    "require": {
        "php": ">=8.0",
        "doctrine/dbal": "^3.0",
        "firebase/php-jwt": "^5.4",
        "hyperf/amqp": "^3.0.0",
        "hyperf/constants": "^3.0.0",
        "hyperf/database": "^3.0.0",
        "hyperf/command": "^3.0.0",
        "hyperf/config": "^3.0.0",
        "hyperf/cache": "^3.0.0",
        "hyperf/config-center": "^3.0.0",
        "hyperf/config-nacos": "^3.0.0",
        "hyperf/db-connection": "^3.0.0",
        "hyperf/di": "^3.0.0",
        "hyperf/engine": "^1.2|^2.8",
        "hyperf/filesystem": "^3.0.0",
        "hyperf/framework": "^3.0.0",
        "hyperf/guzzle": "^3.0.0",
        "hyperf/http-server": "^3.0.0",
        "hyperf/json-rpc": "^3.0.0",
        "hyperf/logger": "^3.0.0",
        "hyperf/redis": "^3.0.0",
        "hyperf/rpc-client": "^3.0.0",
        "hyperf/validation": "^3.0.0",
        "hyperf/xxl-job-incubator": "^0.2.0",
        "hyperf/memory": "^3.0.0",
        "hyperf/model-cache": "^3.0.0",
        "hyperf/paginator": "^3.0.0",
        "hyperf/process": "^3.0.0",
        "hyperf/rpc": "^3.0.0",
        "hyperf/rpc-server": "^3.0.0",
        "hyperf/service-governance": "^3.0.0",
        "hyperf/service-governance-nacos": "^3.0.0",
        "league/flysystem": "^2.3",
        "league/flysystem-aws-s3-v3": "^2.0",
        "overtrue/flysystem-cos": "^4.0",
        "tencentcloud/captcha": "^3.0",
        "viest/php-ext-xlswriter-ide-helper": "^1.3.6",
        "ext-redis": "*"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "~3.5.0",
        "mockery/mockery": "^1.0",
        "phpstan/phpstan": "^1.8",
        "swoole/ide-helper": "^5.0"
    },
    "suggest": {
        "ext-openssl": "Required to use HTTPS.",
        "ext-json": "Required to use JSON."
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "optimize-autoloader": true,
        "sort-packages": true
    },
    "scripts": {
        "test": "phpunit -c phpunit.xml --colors=always",
        "analyse": "phpstan analyse --memory-limit 1024M -l 0 ./src",
        "cs-fix": "php-cs-fixer fix $1"
    },
    "extra": {
        "hyperf": {
            "config": "Bailing\\ConfigProvider"
        }
    },
    "repositories": [
        {
            "description": "华为Composer全量镜像",
            "type": "composer",
            "url": "https://repo.huaweicloud.com/repository/php/",
            "canonical": false
        },
        {
            "description": "官方源",
            "type": "composer",
            "url": "https://repo.packagist.org"
        }
    ]
}
